<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#121323"
    tools:context=".ProteinFragment">

    <View
        android:id="@+id/view2"
        android:layout_width="356dp"
        android:layout_height="120dp"
        android:layout_marginTop="-10dp"
        android:alpha="0.50"
        android:background="#D9D9D9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:fontFamily="@font/mainfont"
        android:letterSpacing="0.3"
        android:textColor="@color/white"
        android:text="PROTEIN"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.538" />

    <Button
        android:id="@+id/back"
        android:layout_width="36dp"
        android:layout_height="44dp"
        android:background="#0000"
        android:text="c"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.817"
        app:layout_constraintStart_toEndOf="@+id/textView11"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.518" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="37dp"
        android:layout_height="38dp"
        android:layout_marginTop="5dp"
        android:src="@drawable/resource_return"
        app:layout_constraintBottom_toBottomOf="@+id/back"
        app:layout_constraintEnd_toEndOf="@+id/back"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/back"
        app:layout_constraintTop_toTopOf="@+id/back"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/egg"
        android:layout_width="129dp"
        android:layout_height="71dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="35dp"
        android:src="@drawable/egg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="72dp"
        android:layout_height="33dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/mainfont"
        android:text="PRODUCT: EGG\nPROTEIN: 11G\nPESOS: 12"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintStart_toEndOf="@+id/egg"
        app:layout_constraintTop_toTopOf="@+id/egg" />

    <ImageView
        android:id="@+id/chicken"
        android:layout_width="129dp"
        android:layout_height="71dp"
        android:layout_marginTop="48dp"
        android:layout_marginStart="60dp"
        android:src="@drawable/chickenbreast"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/egg" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="126dp"
        android:layout_height="32dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/mainfont"
        android:text="PRODUCT: CHICKEN BREAST\nPROTEIN: 172G\nPESOS: 175"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintStart_toEndOf="@+id/chicken"
        app:layout_constraintTop_toTopOf="@+id/chicken" />

    <ImageView
        android:id="@+id/century"
        android:layout_width="129dp"
        android:layout_height="71dp"
        android:layout_marginTop="48dp"
        android:layout_marginStart="60dp"
        android:src="@drawable/centurytuna"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chicken" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="126dp"
        android:layout_height="32dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/mainfont"
        android:text="PRODUCT: CCENTURY TUNA\nPROTEIN: 25G\nPESOS: 35"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintStart_toEndOf="@+id/century"
        app:layout_constraintTop_toTopOf="@+id/century" />

    <ImageView
        android:id="@+id/sardinas"
        android:layout_width="129dp"
        android:layout_height="71dp"
        android:layout_marginTop="48dp"
        android:layout_marginStart="60dp"
        android:src="@drawable/sardines"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/century" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="126dp"
        android:layout_height="32dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/mainfont"
        android:text="PRODUCT: SARDINAS\nPROTEIN: 155G\nPESOS: 19"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintStart_toEndOf="@+id/sardinas"
        app:layout_constraintTop_toTopOf="@+id/sardinas" />

    <ImageView
        android:id="@+id/san_marino"
        android:layout_width="129dp"
        android:layout_height="71dp"
        android:layout_marginTop="48dp"
        android:layout_marginStart="60dp"
        android:src="@drawable/sanmarino"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sardinas" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="126dp"
        android:layout_height="32dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="18dp"
        android:fontFamily="@font/mainfont"
        android:text="PRODUCT: SAN MARINO\nPROTEIN: 100G\nPESOS: 20"
        android:textColor="@color/white"
        android:textSize="8dp"
        app:layout_constraintStart_toEndOf="@+id/san_marino"
        app:layout_constraintTop_toTopOf="@+id/san_marino" />

</androidx.constraintlayout.widget.ConstraintLayout>